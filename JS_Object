    <script>
        var tutorial = {
            "topic": "javascript",
            "trainer": "vivek",
            "mode":"online"
        }
        document.write("Topic: "+tutorial.topic+"<br>");
        document.write("trainer: "+tutorial.trainer+"<br>");
        document.write("mode: "+tutorial.mode+"<br><br>");

        txt = "";
        var x;
        for (x in tutorial) {
            txt+=tutorial[x]+', '
        }
        document.write(txt);
    </script>
