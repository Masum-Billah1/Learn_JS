<body>
    <script>
        var i;
        var num = parseInt(prompt("Enter The number: ","Enter Number"))
        var prod = 1;
        for(i=2;i<=num;i++)
            prod *=i;
        
        document.write("Factorial of "+num+"is : "+prod);
    </Script>
</body>
